{
    "manifest_version": 2,

    "name": "Chrome Plus",
    "description": "Reimagine your web experience",
    "version": "1.12",

    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["jquery.js", "tripleclick.js", "mousedown.js"]
        }
    ],

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "https://ajax.googleapis.com/"]
}
